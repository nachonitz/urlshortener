{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "ts-node server.ts",
    "dev": "nodemon --watch src --exec ts-node src/server.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d src/infrastructure/typeorm/typeorm.config.ts",
    "typeorm:generate-migration": "npm run typeorm -- -d src/infrastructure/typeorm/typeorm.config.ts migration:generate ./migrations/%npm_config_name%",
    "typeorm:create-migration": "npm run typeorm -- migration:create ./migrations/%npm_config_name%",
    "typeorm:revert-migration": "npm run typeorm -- -d src/infrastructure/typeorm/typeorm.config.ts migration:revert",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mysql2": "^3.11.3",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.24.7",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/supertest": "^6.0.2",
    "babel-jest": "^29.7.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "form-data": "^4.0.0",
    "handlebars": "^4.7.8",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.4",
    "reflect-metadata": "^0.2.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
